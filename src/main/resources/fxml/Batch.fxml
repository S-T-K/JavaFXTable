<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar fx:id="batchMenu" layoutX="1.0" prefHeight="26.0" prefWidth="243.0" AnchorPane.leftAnchor="-10.0" AnchorPane.rightAnchor="-10.0" AnchorPane.topAnchor="-10.0">
         <menus>
            <Menu mnemonicParsing="false" text="Files">
               <items>
                  <MenuItem fx:id="addReferenceMzXML" mnemonicParsing="false" onAction="#openBatchmzxmlChooser" text="Add new .mzXML" />
                  <Menu mnemonicParsing="false" text="Shift">
                     <items>
                        <MenuItem fx:id="calculate" mnemonicParsing="false" onAction="#newwindowcalculate" text="Calculate Shift" />
                        <MenuItem mnemonicParsing="false" text="Change Penalty">
                           <graphic>
                              <TextField fx:id="batsetpen" maxWidth="40.0" />
                           </graphic>
                        </MenuItem>
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Rename">
                     <items>
                        <MenuItem mnemonicParsing="false">
                           <graphic>
                              <TextField fx:id="paneName" />
                           </graphic>
                        </MenuItem>
                     </items>
                  </Menu>
                  <MenuItem fx:id="delteBatch" mnemonicParsing="false" onAction="#deleteBatch" text="Delete Batch" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <Menu mnemonicParsing="false" text="Color">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Set default color">
                           <graphic>
                              <ColorPicker fx:id="batdefcol" maxWidth="40.0" prefWidth="40.0" />
                           </graphic>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Change color">
                           <graphic>
                              <ColorPicker fx:id="batsetcol" maxWidth="40.0" prefWidth="40.0" />
                           </graphic>
                        </MenuItem>
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Line width">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Set default width">
                           <graphic>
                              <TextField fx:id="batdefwidth" maxWidth="40.0" prefWidth="40.0" />
                           </graphic>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Change width">
                           <graphic>
                              <TextField fx:id="batsetwidth" maxWidth="40.0" prefWidth="40.0" />
                           </graphic>
                        </MenuItem>
                     </items>
                  </Menu>
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <TableView fx:id="batchFileView" editable="true" layoutY="16.0" onMouseClicked="#changedFile" prefHeight="234.0" prefWidth="243.0" AnchorPane.bottomAnchor="-10.0" AnchorPane.leftAnchor="-10.0" AnchorPane.rightAnchor="-10.0" AnchorPane.topAnchor="16.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="fileColumn" prefWidth="199.9999634027481" text="File">
               <cellValueFactory>
                  <PropertyValueFactory property="File" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colorColumn" maxWidth="57.0" minWidth="57.0" prefWidth="57.0" text="Color" />
            <TableColumn fx:id="widthColumn" maxWidth="57.0" minWidth="0.0" prefWidth="0.0" text="Width">
               <cellValueFactory>
                  <PropertyValueFactory property="Width" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="shiftColumn" maxWidth="77.0" minWidth="0.0" prefWidth="0.0" text="Shift [ppm]">
               <cellValueFactory>
                  <PropertyValueFactory property="mzshift" />
               </cellValueFactory>
            </TableColumn>
         </columns>
         <contextMenu>
            <ContextMenu onShowing="#checkforFile">
              <items>
                  <MenuItem fx:id="deleteFile" mnemonicParsing="false" onAction="#deleteFile" text="Delete file" />
                  <MenuItem mnemonicParsing="false" onAction="#openBatchmzxmlChooser" text="Add new .mzXML" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
   </children>
</fx:root>
